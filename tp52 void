#include<bits/stdc++.h>
using namespace std;

void mostrarTablaMultiplicar(int N); 

int main() {
    int N;
    cout << "Ingrese un entero N: ";
    cin >> N;

    mostrarTablaMultiplicar(N);

    return 0;
}

void mostrarTablaMultiplicar(int N) {
    for (int i = 1; i <= 10; i++) {
        int resultado = i * N;
        cout << i << " x " << N << " = " << resultado << endl;
    }
}
